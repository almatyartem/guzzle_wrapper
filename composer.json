{
    "name": "turbodev/guzzle_wrapper",
    "description": "Guzzle wrapper",
    "repositories":[
        {
            "type": "vcs",
            "url": "git@github.com:almatyartem/response_wrapper.git"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "turbodev/response_wrapper": "dev-master",
        "guzzlehttp/guzzle": "^7.2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "src"
        ],
        "psr-4": {
            "GuzzleWrapper\\": "src/"
        }
    }
}
